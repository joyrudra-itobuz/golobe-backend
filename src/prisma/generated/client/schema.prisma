generator client {
  provider = "prisma-client-js"
  output   = "./generated/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

// Use UUID as the default ID
model User {
  id    String    @id @default(dbgenerated()) @db.Uuid
  name  String
  email String  @unique
  password String 
}
